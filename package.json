        "analyze": "NODE_ENV=production webpack --config ./script/webpack.analyze.js"
    },
    "dependencies": {
        "core-js": "^2.6.5",
        "dat.gui": "^0.7.6",
        "vue": "^2.6.10"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.11.0",
        "@vue/cli-plugin-eslint": "^3.11.0",
        "autoprefixer": "^9.6.4",
        "babel-eslint": "^10.0.1",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^5.0.4",
        "cross-env": "^6.0.3",
        "css-loader": "^3.2.0",
        "eslint": "^5.16.0",
        "eslint-plugin-vue": "^5.0.0",
        "file-loader": "^4.2.0",
        "html-webpack-plugin": "^3.2.0",
        "mini-css-extract-plugin": "^0.8.0",
